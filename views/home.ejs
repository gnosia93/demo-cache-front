        <script>
            $( function() {
                $( "#dialog" ).dialog({
                    autoOpen: false,
                    width: '500px',
                    focus: false,
                    show: {
                        effect: "blind",
                        duration: 500
                    },
                    hide: {
                        effect: "explode",
                        duration: 1000
                    }
                });

                $( "#opener" ).on( "click", function() {
                    $( "#dialog" ).dialog( "open" );
                });
            } );
        </script>
    </head>
    <body>


        
        <script>
            function timedRefresh(timeoutPeriod) {
                setTimeout("location.reload(true);",timeoutPeriod);
            }
                
